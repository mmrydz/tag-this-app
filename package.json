{
  "name": "tag-this-db",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jsdom",
    "start:prod": "node server/server.js",
    "start:dev": "concurrently \"nodemon server/server.js --ignore 'client/*'\" \"npm run client\"",
    "api": "nodemon ./server/server.js"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "aws-amplify": "^1.1.28",
    "aws-sdk": "^2.458.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.4",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "express-session": "^1.15.6",
    "file-type": "^11.0.0",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "morgan": "^1.9.0",
    "multiparty": "^4.2.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.6.3",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.6.3",
    "react-iframe": "^1.8.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "serve-static": "^1.14.1",
    "spectre.css": "^0.5.0"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
